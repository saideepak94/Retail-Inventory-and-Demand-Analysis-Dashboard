    "# Cell 1 - imports & notebook settings\n",
    "%matplotlib inline\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "plt.rcParams['figure.figsize'] = (10,5)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "c92282c0",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Cell 2 - load datasets\n",
    "inv = pd.read_csv('inventory.csv')\n",
    "sales = pd.read_csv('sales.csv')\n",
    "sup = pd.read_csv('supplier.csv')\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "6d08e020",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Cell 3 - quick preprocessing\n",
    "sales['date'] = pd.to_datetime(sales['date'])\n",
    "sales['revenue'] = sales['units_sold'] * sales['selling_price']\n",
    "inv['purchase_price'] = pd.to_numeric(inv['purchase_price'], errors='coerce')\n",
    "inv['current_stock'] = pd.to_numeric(inv['current_stock'], errors='coerce')\n",
    "inv['reorder_level'] = pd.to_numeric(inv['reorder_level'], errors='coerce')\n",
    "sup['delivery_time_days'] = pd.to_numeric(sup['delivery_time_days'], errors='coerce')\n",
    "sup['defect_rate'] = pd.to_numeric(sup['defect_rate'], errors='coerce')\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "4eb51ba5",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Cell 4 - create master merged dataset (product-level & supplier)\n",
    "# Aggregate sales to product-level\n",
    "prod_sales = sales.groupby('product_id').agg(\n",
    "    total_units_sold=('units_sold','sum'),\n",
    "    total_revenue=('revenue','sum'),\n",
    "    avg_selling_price=('selling_price','mean'),\n",
    "    sales_count=('sale_id','count')\n",
    ").reset_index()\n",
    "\n",
    "master = inv.merge(prod_sales, on='product_id', how='left')\n",
    "master = master.merge(sup[['supplier_id','supplier_name','delivery_time_days','defect_rate']],\n",
    "                      on='supplier_id', how='left')\n",
    "\n",
    "master['total_units_sold'] = master['total_units_sold'].fillna(0).astype(int)\n",
    "master['total_revenue'] = master['total_revenue'].fillna(0)\n",
    "master['avg_selling_price'] = master['avg_selling_price'].fillna(0)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "88ea0721",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Cell 5 - helper plotting functions\n",
    "def save_show(fig, title=None):\n",
    "    if title:\n",
    "        fig.suptitle(title, fontsize=12)\n",
    "    plt.show()\n",
    "\n",
    "def bar_plot(x, y, xlabel, ylabel, title, rotate=45):\n",
    "    fig, ax = plt.subplots()\n",
    "    ax.bar(x, y)\n",
    "    ax.set_xlabel(xlabel)\n",
    "    ax.set_ylabel(ylabel)\n",
    "    ax.tick_params(axis='x', rotation=rotate)\n",
    "    fig.tight_layout()\n",
    "    save_show(fig, title)\n",
    "\n",
    "def line_plot(x, y, xlabel, ylabel, title):\n",
    "    fig, ax = plt.subplots()\n",
    "    ax.plot(x, y, marker='o')\n",
    "    ax.set_xlabel(xlabel)\n",
    "    ax.set_ylabel(ylabel)\n",
    "    fig.tight_layout()\n",
    "    save_show(fig, title)\n",
    "\n",
    "def scatter_plot(x, y, xlabel, ylabel, title):\n",
    "    fig, ax = plt.subplots()\n",
    "    ax.scatter(x, y, alpha=0.6)\n",
    "    ax.set_xlabel(xlabel)\n",
    "    ax.set_ylabel(ylabel)\n",
    "    fig.tight_layout()\n",
    "    save_show(fig, title)\n"
   ]
  },
