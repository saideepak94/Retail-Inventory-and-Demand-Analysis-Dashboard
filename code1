   "id": "03d812b2",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Products where historical total sales > current stock: 91\n"
     ]
    },
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 1000x500 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "# Q13: How often do sales exceed available stock? (histor check)\n",
    "# Create total historical units_sold per product and compare to current stock\n",
    "historical = prod_sales[['product_id','total_units_sold']].merge(inv[['product_id','current_stock']], on='product_id', how='left')\n",
    "historical['sold_vs_stock_ratio'] = historical['total_units_sold'] / (historical['current_stock'].replace(0, np.nan))\n",
    "# count how many products historically sold more than current stock (indicator of possible stockouts if restock not done)\n",
    "exceed_count = (historical['total_units_sold'] > historical['current_stock']).sum()\n",
    "print(\"Products where historical total sales > current stock:\", exceed_count)\n",
    "hist_vals = historical['sold_vs_stock_ratio'].replace([np.inf, -np.inf], np.nan).dropna()\n",
    "fig, ax = plt.subplots()\n",
    "ax.hist(hist_vals, bins=30)\n",
    "ax.set_xlabel('historical total units sold / current stock')\n",
    "ax.set_ylabel('count')\n",
    "ax.set_title('Q13 - Distribution of historical sales vs current stock')\n",
    "plt.show()\n"
   ]
